   "source": [
    "k sources, l rounds (k <= l)\n",
    "\n",
    "Li: 除去 i - 1 round leaves后剩下的node i<= l\n",
    "Bi: 距离burned过的nodes为1的nodes\n",
    "\n",
    "R1 = L1/B1 \n",
    "...\n",
    "Rl = Ll/Bl\n"
   ]
  },
  {
